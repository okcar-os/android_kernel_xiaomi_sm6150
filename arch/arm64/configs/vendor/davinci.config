# Compass
CONFIG_AKM09970=y

# Device
CONFIG_MACH_XIAOMI_DAVINCI=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_GOODIX_TA=y

# Ramoops
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"

# SKU
CONFIG_CMDLINE_HWC_IS_PRODUCT_SKU=y

# Step motor
CONFIG_TI_DRV8846=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_GTX8=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y
CONFIG_TOUCHSCREEN_TDDI_DBCLK=y

CONFIG_IPV6_ILA=y
CONFIG_IPV6_MROUTE=y
CONFIG_IPV6_PIMSM_V2=y
CONFIG_USB_NET_CDC_NCM=m
CONFIG_USB_OKCAR=y
